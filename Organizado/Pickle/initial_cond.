# Normandy - Hospitalizations

# Initial Conditions

def initial_cond_0(y_t):
    #A0 = 100
    A0 = 1*max(y_t)
    tp0 = (2/3)*len(y_t)
    delta0 = (1/4)*len(y_t)
    nu0 = 1
    return [A0, tp0, delta0, nu0]

optimize.initial_cond = initial_cond_0

def update_cond_nw(A0, tp0):
    tp0 += 110 
    A0 *= 0.005
    return [A0, tp0]
optimize.update_cond = update_cond_nw

